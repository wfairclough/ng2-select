{
  "name": "ng2-select-base",
  "version": "3.0.0",
  "private": true,
  "description": "Angular2 based replacement for select boxes",
  "scripts": {
    "link": "ngm link -p src --here",
    "build": "ngm build -p src --clean",
    "build.watch": "ngm build -p src --watch --skip-bundles",
    "start": "ng serve --aot",
    "pretest": "run-s lint build link",
    "test": "ng test -sr"
  },
  "main": "dist/index.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
